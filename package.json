{
  "name": "pages-gatsby",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-serve-open": "npm run build && npm run serve",
    "deploy": "npm run build && gh-pages -d public -b master -r https://github.com/kkrnme/kkrnme.github.io",
    "extreme-deploy": "npm run format && npm run refresh  && npm run deploy",
    "extreme-develop": "npm run format && npm run refresh && npm run develop",
    "develop": "gatsby develop --host 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,pcss}\"",
    "watch": "npm run format && onchange 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.pcss'  -- prettier --write {{changed}}",
    "refresh": "rm -rfd .cache public",
    "start": "npm run develop",
    "serve": "gatsby serve -p 8000 -o",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@mdx-js/mdx": "^1.5.2",
    "@mdx-js/react": "^1.5.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.20",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/twemoji": "^12.1.0",
    "csstype": "^2.6.8",
    "gatsby": "^2.18.13",
    "gatsby-plugin-emotion": "^4.1.18",
    "gatsby-plugin-graphql-codegen": "^2.0.1",
    "gatsby-plugin-mdx": "^1.0.62",
    "gatsby-plugin-postcss": "^2.1.18",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-sharp": "^2.3.9",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-remark-images": "^3.1.38",
    "gatsby-source-filesystem": "^2.1.42",
    "geopattern": "^1.2.3",
    "lodash": "^4.17.15",
    "postcss-scss": "^2.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-twemoji": "^0.2.3",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.4",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "graphql": "^14.5.8",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "typescript-styled-plugin": "^0.15.0"
  }
}
