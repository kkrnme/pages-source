{
  "name": "pages-gatsby",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "deploy": "npm run build && gh-pages -d public -b master -r https://github.com/kkrnme/kkrnme.github.io",
    "extreme-deploy": "npm run format && npm run refresh  && npm run deploy",
    "extreme-develop": "npm run format && npm run refresh && npm run develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,pcss}\"",
    "watch": "onchange 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.pcss'  -- prettier --write {{changed}}",
    "refresh": "rm -rfd .cache public",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/twemoji": "^12.1.0",
    "gatsby": "^2.18.2",
    "gatsby-plugin-emotion": "^4.1.15",
    "gatsby-plugin-mdx": "^1.0.57",
    "gatsby-plugin-postcss": "^2.1.15",
    "gatsby-plugin-typescript": "^2.1.19",
    "gatsby-remark-emoji-unicode": "^0.1.0",
    "gatsby-source-filesystem": "^2.1.38",
    "lodash": "^4.17.15",
    "postcss-scss": "^2.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-twemoji": "^0.2.3",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.3",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "graphql": "^14.5.8",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1"
  }
}
