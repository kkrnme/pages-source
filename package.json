{
  "name": "pages-gatsby",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-serve": "npm run build && npm run serve",
    "deploy": "npm run build && gh-pages -d public -b master -r https://github.com/kkrnme/kkrnme.github.io",
    "extreme-deploy": "npm run format && npm run refresh  && npm run deploy",
    "extreme-develop": "npm run format && npm run refresh && npm run develop",
    "develop": "gatsby develop --host 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,pcss}\"",
    "watch": "npm run format && onchange 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.pcss'  -- prettier --write {{changed}}",
    "clean": "gatsby clean",
    "open": "gatsby serve -p 8000 -o --host 0.0.0.0",
    "start": "npm run develop",
    "serve": "gatsby serve -p 8000 --host 0.0.0.0",
    "test": "jest"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@mdx-js/mdx": "^1.5.4",
    "@mdx-js/react": "^1.5.4",
    "@types/node": "^13.1.7",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "@types/twemoji": "^12.1.0",
    "csstype": "^2.6.8",
    "gatsby": "^2.18.23",
    "gatsby-background-image": "^0.9.12",
    "gatsby-plugin-catch-links": "^2.1.23",
    "gatsby-plugin-emotion": "^4.1.20",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-graphql-codegen": "^2.1.1",
    "gatsby-plugin-manifest": "^2.2.36",
    "gatsby-plugin-mdx": "^1.0.66",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-postcss": "^2.1.18",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.1.20",
    "gatsby-plugin-sharp": "^2.3.12",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-plugin-typescript": "^2.1.25",
    "gatsby-remark-autolink-headers": "^2.1.21",
    "gatsby-remark-images": "^3.1.41",
    "gatsby-source-filesystem": "^2.1.45",
    "gatsby-transformer-sharp": "^2.3.11",
    "geopattern": "^1.2.3",
    "postcss": "^7.0.26",
    "postcss-scss": "^2.0.0",
    "purgecss": "^1.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-twemoji": "^0.2.3",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.4",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/react-test-renderer": "^16.9.1",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.26",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gh-pages": "^2.2.0",
    "graphql": "^14.5.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "typescript-styled-plugin": "^0.15.0"
  }
}
